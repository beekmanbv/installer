{
    "name": "beekmanbv/installer",
    "description": "Beekman - Laravel application installer.",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/filesystem": "^10.20",
        "illuminate/support": "^10.20",
        "laravel/prompts": "^0.1",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0"
    },
    "bin": [
        "bin/beekman"
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Installer\\Console\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
